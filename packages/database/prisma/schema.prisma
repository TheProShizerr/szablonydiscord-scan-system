generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Template {
  id Int @id @default(autoincrement())
  templateId String
  title String
  usage Int
  dateCreate String
}

model User {
  id Int @id @default(autoincrement())
  avatar String 
  userId String @unique
  username String
  template Templates[]
}

model Templates {
  in Int @id @default(autoincrement())
  templateId String
  categories String
  dateCreate String
  link String
  title String
  description String?
  usageCount Int
  clickButtonUse Int?
  server String?
  serverLink String?
  authorId String
  author User @relation(fields: [authorId], references: [userId])
}